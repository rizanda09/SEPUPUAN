<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.sepupuan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SEPUPUAN">
        <activity android:name=".PenginapanActivity"
            android:label="@string/penginapan"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".DetailpenginapanActivity"
            android:label="@string/dtlpenginapan"
            android:parentActivityName=".PenginapanActivity"/>
        <activity android:name=".DetailkulinerActivity"
            android:label="@string/dtlkuliner"
            android:parentActivityName=".KulinerActivity"/>
        <activity android:name=".KulinerActivity"
            android:label="@string/kuliner"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".DetailwisataActivity"
            android:label="@string/dtlwisata"
            android:parentActivityName=".WisataActivity"/>
        <activity android:name=".WisataActivity"
            android:label="@string/wisata"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name=".LoginActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name=".MainActivity" />
    </application>

</manifest>